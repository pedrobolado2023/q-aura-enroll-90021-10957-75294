{
  "type": "static",
  "build": {
    "buildCommand": "npm install && npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "nodeVersion": "18"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_VERSION": "18",
    "VITE_SUPABASE_URL": "$VITE_SUPABASE_URL",
    "VITE_SUPABASE_PUBLISHABLE_KEY": "$VITE_SUPABASE_PUBLISHABLE_KEY"
  }
}